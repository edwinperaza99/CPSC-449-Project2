{
	"$schema": "https://www.krakend.io/schema/v2.4/krakend.json",
	"version": 3,
	"endpoints": [
		{
			"endpoint": "/api/classes/",
			"method": "GET",
			"input_query_strings": ["department_name"],
			"backend": [
				{
					"url_pattern": "/classes",
					"host": ["http://localhost:5000"]
				}
			]
		},
        {
            "endpoint": "/api/classes/",
            "method": "POST",
            "backend": [
                {
                    "url_pattern": "/classes",
                    "host": ["http://localhost:5000"],
                    "extra_config": {
						"backend/http": {
							"return_error_details": "krakend_backend"
						}
					}
                }
            ]
        },
        {
            "endpoint": "/api/enrollment/",
            "method": "POST",
